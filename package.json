{
  "name": "fullstack-react-app",
  "description": "fullstack react app boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
		"start": "nodemon server.js",
		"dev": "nodemon server.js & webpack -w",
		"build": "webpack --config ./webpack.config.js",
    "lint": "eslint . --ext .js,.jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikkel7emil/fullstack-react-app.git"
  },
  "keywords": [
    "react",
    "nodejs",
    "javascript",
    "server",
    "fullstack",
    "express"
  ],
  "author": "mikkel larsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikkel7emil/fullstack-react-app/issues"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "homepage": "https://github.com/mikkel7emil/fullstack-react-app#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "^4.16.3",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "nodemon": "^1.18.4",
    "pre-commit": "^1.2.2",
    "serialize-javascript": "^1.4.0",
    "style-loader": "^0.20.3",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "pre-commit": [
    "lint"
  ]
}
